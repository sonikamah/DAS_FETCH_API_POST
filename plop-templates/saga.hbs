
import { call, put, takeLatest } from 'redux-saga/effects';
import {delay} from 'redux-saga';
import * as actionTypes from './constants';
import apiMethod from './api';

// Please implement as per the need
export function* myMethodName() {
    try {
        let data = yield call(apiMethod);
        yield [
            put({ type: actionTypes.SAGA_SUCCESS, questions: data}),
        ];
    } catch (error) {
       yield put({ type: actionTypes.SAGA_ERROR, questions: error });
    }
}

export default function* rootSaga() {
    yield takeLatest(actionTypes.SAGA_ACTION, myMethodName);
}